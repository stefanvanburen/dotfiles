# vim: set filetype=ghostty:

# the default; some faces
font-size = 10

# https://qwerasd205.github.io/PixelCode/
font-family = "Pixel Code"
font-size = 8

# https://berkeleygraphics.com/typefaces/berkeley-mono/
font-family = "Berkeley Mono Variable"

# https://github.com/be5invis/Iosevka
font-family = "Iosevka Term"

# https://practicaltypography.com/triplicate.html
font-family = "Triplicate A Code"

# https://go.dev/blog/go-fonts
font-family = "Go Mono"

# https://github.com/fabrizioschiavi/pragmatapro
font-family = "PragmataPro Mono Liga"
font-family = "PragmataPro VF Mono Liga"

# https://github.com/eigilnikolajsen/commit-mono
font-family = "CommitMono"

# https://ia.net/topics/a-typographic-christmas
font-family = "iA Writer Mono S"

# https://input.djr.com/
font-family = "Input Mono Compressed"

# https://github.com/arrowtype/recursive
font-family = "Rec Mono Linear"

## https://github.com/githubnext/monaspace
font-family = "Monaspace Argon Var"
font-family = "Monaspace Krypton Var"
font-family = "Monaspace Neon Var"
font-family = "Monaspace Radon Var"
